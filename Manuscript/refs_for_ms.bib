	File = {ScienceDirect Snapshot:/Users/rpk/Library/Application Support/Firefox/Profiles/p3bkhpla.default/zotero/storage/5N8NS8XU/S000632071400442X.html:text/html},
	Issn = {0006-3207},
	Journal = {Biological Conservation},
	Keywords = {Asian carp, Environmental DNA, Fishes, Genetic monitoring, Invasive species, Noninvasive genetic sampling, Rare species, Sediment, Water},
	Month = mar,
	Pages = {93--102},
	Series = {Special {Issue}: {Environmental} {DNA}: {A} powerful new tool for biological conservation},
	Title = {Fish environmental {DNA} is more concentrated in aquatic sediments than surface water},
