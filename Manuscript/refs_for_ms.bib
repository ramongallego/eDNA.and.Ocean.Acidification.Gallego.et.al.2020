	Doi = {10.1890/13-0996.1},
	Issn = {0012-9658},
	Journal = {Ecology},
	Month = dec,
	Number = {12},
	Pages = {2663--2669},
	Title = {Quantifying effects of abiotic and biotic drivers on community dynamics with multivariate autoregressive ({MAR}) models},
	Url = {http://www.esajournals.org/doi/abs/10.1890/13-0996.1},
	Urldate = {2015-07-10},
