	File = {Full Text PDF:/Users/rpk/Library/Application Support/Firefox/Profiles/p3bkhpla.default/zotero/storage/HNUEWHKQ/Berlow et al. - 2004 - Interaction strengths in food webs issues and opp.pdf:application/pdf},
	Issn = {1365-2656},
	Journal = {Journal of Animal Ecology},
