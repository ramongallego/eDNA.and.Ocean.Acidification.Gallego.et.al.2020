	File = {Snapshot:/Users/rpk/Library/Application Support/Firefox/Profiles/p3bkhpla.default/zotero/storage/4ST685N2/full.html:text/html},
	Issn = {1365-2656},
	Journal = {Journal of Animal Ecology},
	Keywords = {amphipod, composition, diversity, food web, herbivore, immigration, isopod, marine, predator, prey, richness},
	Language = {en},
	Month = jul,
	Number = {4},
	Pages = {732--740},
	Title = {Predator richness has no effect in a diverse marine food web},
	Url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1365-2656.2009.01533.x/abstract},
	Urldate = {2015-07-06},
